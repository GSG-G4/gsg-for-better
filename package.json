{
  "name": "gsg-for-better",
  "version": "0.0.0",
  "private": false,
  "jest": {
    "testRegex": "server/__test__/*"
  },
  "scripts": {
    "start": "cd server && cross-env DEBUG=gfb node ./index",
    "server:dev": "cd server && cross-env DEBUG=gfb nodemon ./index",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server:test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "build-db": "cross-env DEBUG=database node server/database/fake_data/index.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-favicon": "^2.0.1",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "joi": "^14.3.0",
    "mongoose": "^5.4.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.3.0",
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.14.0",
    "eslint-config-prettier": "^2.9.0"
  }
}
